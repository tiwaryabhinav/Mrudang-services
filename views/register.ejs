	<!-- main -->
	<div class="main-w3layouts wrapper">
		<h1 style="color: #6252FF;">Register at Mrudang</h1>
		<div class="main-agileinfo">
			<div class="agileits-top">
				<form action="/signup" method="POST" enctype="multipart/form-data">
					<input type="text" placeholder="Name" class="form__input" name="name" />
					<input type="number" placeholder="Age" min="0" class="form__input" name="age" />
					<input type="text" placeholder="Address" class="form__input" name="address" />
					<input type="text" placeholder="State" class="form__input" name="state" />
					<input list="crop" placeholder="Choose your crop type:" name="Produce_type">
					<datalist id="crop">
						<option value="Kharif">
						<option value="Rabi">
					</datalist>
					<input type="number" placeholder="Produce Quantity" min="0" class="form__input"
						name="Produce_quantity" />
					<br>
					<input type="number" placeholder="Phone" min="0" class="form__input" name="Phone" />
					<input type="email" placeholder="Email" class="form__input" name="email" />
					<input type="text" placeholder="Estimated Price" min="0" class="form__input"
						name="estimated_price" />
					<input type="text" placeholder="Tell us more..." min="0" class="form__input input-lg" name="info" / style="height: 100px;">
					<input type="password" placeholder="Password" class="form__input" name="Password" />
					<div>
						<label for="file">Upload a profile picture</label>
						<input type="file" id="file" name="avatar">
					</div>

					<div class="wthree-text">
						<label class="anim">
							<input type="checkbox" class="checkbox" required="">
							<span>I Agree To The Terms & Conditions</span>
						</label>
						<div class="clear"> </div>
					</div>
					<input type="submit" value="SIGNUP" style="color: white;">
				</form>
				<p>Don't have an Account? <a href="/login"> Login Now!</a></p>
			</div>
